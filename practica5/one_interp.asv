
function [r] = one_interp(mat, n)
    kernel = ones(1, n+1);
    kernel(n+1)=0;
    ra = zero_fill_interp(mat, n);
    numRows = size(ra, 1);
    r = zeros(size(ra, 1)+n, size(ra, 2)+n);
    for i=1:numRows
       if i==1
            aux = conv(kernel, mat(i));
            r(i)= aux;
       elseif mod(i, n)==0           
           aux = conv(kernel, mat(i));
           
       else
           mat(i, )=0;
           mat(i)=aux;
       end
    end
end